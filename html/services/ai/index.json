  "integrations": {
    "api": {
      "enabled": true,
      "endpoints": [
        {
          "service": "ollama",
          "endpoint": "/api/generate",
          "method": "POST"
        },
        {
          "service": "whisper", 
          "endpoint": "/asr",
          "method": "POST"
        }
      ]
    },
    "workflows": {
      "textToImage": {
        "steps": ["stable-diffusion", "comfyui"],
        "description": "Generate images from text prompts"
      },
      "audioTranscription": {
        "steps": ["whisper"],
        "description": "Convert audio to text"
      },
      "codeGeneration": {
        "steps": ["openwebui", "codeassist"],
        "description": "Generate and improve code"
      }
    }
  },
  "requirements": {
    "hardware": {
      "gpu": {
        "recommended": "NVIDIA RTX 4090",
        "minimum": "NVIDIA GTX 1060 6GB",
        "memory": "8GB VRAM minimum, 24GB recommended"
      },
      "ram": "16GB minimum, 32GB recommended",
      "storage": "100GB for models and cache"
    },
    "software": {
      "docker": "20.10+",
      "nvidia-docker": "Required for GPU acceleration",
      "python": "3.8+ (for development)"
    }
  },
  "models": {
    "downloadLocation": "/opt/ai-models",
    "autoDownload": false,
    "popularModels": [
      {
        "name": "Llama 3.1 8B",
        "size": "4.7GB",
        "type": "Text Generation",
        "compatibility": ["ollama", "openwebui"]
      },
      {
        "name": "Stable Diffusion XL",
        "size": "6.9GB", 
        "type": "Image Generation",
        "compatibility": ["stable-diffusion", "comfyui"]
      },
      {
        "name": "Whisper Large V3",
        "size": "1.5GB",
        "type": "Speech Recognition", 
        "compatibility": ["whisper"]
      }
    ]
  },
  "security": {
    "apiKeys": {
      "required": true,
      "storage": "environment_variables",
      "rotation": "monthly"
    },
    "rateLimiting": {
      "enabled": true,
      "requestsPerMinute": 60,
      "requestsPerHour": 1000
    },
    "accessControl": {
      "ipWhitelist": true,
      "userAuthentication": "optional"
    }
  },
  "monitoring": {
    "metrics": [
      "requests_per_second",
      "response_time",
      "gpu_utilization", 
      "memory_usage",
      "model_loading_time"
    ],
    "alerts": [
      {
        "condition": "gpu_memory > 90%",
        "action": "notification"
      },
      {
        "condition": "response_time > 30s",
        "action": "restart_service"
      }
    ]
  },
  "troubleshooting": {
    "commonIssues": [
      {
        "issue": "GPU out of memory",
        "solutions": [
          "Reduce batch size",
          "Use smaller models",
          "Clear GPU cache",
          "Restart services"
        ]
      },
      {
        "issue": "Model loading fails",
        "solutions": [
          "Check disk space",
          "Verify model file integrity",
          "Check network connectivity",
          "Review Docker logs"
        ]
      },
      {
        "issue": "Slow inference",
        "solutions": [
          "Enable GPU acceleration",
          "Optimize model parameters",
          "Check system resources",
          "Use quantized models"
        ]
      }
    ],
    "logs": {
      "ollama": "/var/log/ollama/",
      "stable-diffusion": "/var/log/sd-webui/",
      "whisper": "/var/log/whisper/"
    }
  },
  "backups": {
    "critical": [
      "Model configurations",
      "Custom prompts",
      "User settings",
      "API keys"
    ],
    "optional": [
      "Chat history",
      "Generated images",
      "Audio transcriptions"
    ],
    "exclude": [
      "Model files (too large)",
      "Temporary cache",
      "Log files"
    ]
  }
}