  "integrations": {
    "homeAssistant": {
      "enabled": true,
      "entities": [
        "media_player.emby",
        "media_player.plex"
      ]
    },
    "monitoring": {
      "prometheus": true,
      "grafana": true
    }
  },
  "troubleshooting": {
    "commonIssues": [
      {
        "issue": "Service not accessible",
        "solutions": [
          "Check if Docker container is running",
          "Verify port forwarding",
          "Check firewall settings",
          "Confirm reverse proxy configuration"
        ]
      },
      {
        "issue": "Slow streaming",
        "solutions": [
          "Check network bandwidth",
          "Verify hardware transcoding",
          "Optimize media bitrates",
          "Check storage performance"
        ]
      }
    ],
    "logs": {
      "emby": "/var/lib/emby/logs/",
      "jellyfin": "/var/log/jellyfin/",
      "plex": "/var/lib/plexmediaserver/Library/Application Support/Plex Media Server/Logs/"
    }
  },
  "backups": {
    "recommended": [
      "Configuration files",
      "Database backups",
      "User preferences",
      "Custom artwork"
    ],
    "exclude": [
      "Cache directories",
      "Temporary files",
      "Media files (unless specifically needed)"
    ]
  }
